{
  "name": "@isardsat/tri",
  "version": "1.12.3",
  "description": "Powerful configuration file processor",
  "keywords": [
    "configuration",
    "yaml",
    "csv"
  ],
  "homepage": "https://github.com/lobelia-earth/tri",
  "bugs": {
    "url": "https://github.com/lobelia-earth/tri/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lobelia-earth/tri.git"
  },
  "license": "MIT",
  "author": "Lobelia",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/cjs/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc && tsc -p tsconfig.cjs.json",
    "build:watch": "tsc --watch & tsc --watch -p tsconfig.cjs.json",
    "clean": "rm -r lib",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"src/**/*.[jt]s\"",
    "test": "jest --coverage --verbose 2>&1",
    "typecheck": "tsc --pretty",
    "xxl": "xxl --src src --exclude /coverage/"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "testRegex": ".*__tests__/.*\\.test\\.(ts|tsx)$"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "mustache": "^4.0.1",
    "papaparse": "^5.3.2",
    "timm": "^1.6.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/js-yaml": "^4.0.9",
    "@types/mustache": "^4.0.1",
    "@types/papaparse": "^5.3.7",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^26.4.2",
    "prettier": "^2.5.1",
    "typescript": "^4.6.2",
    "xxl": "^1.3.0"
  },
  "volta": {
    "node": "20.4.0",
    "yarn": "1.22.17"
  }
}
